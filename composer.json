{
    "name": "pecserke/universal-mappings",
    "type": "symfony-bundle",
    "description": "Symfony2 Twig DoctrineLoader Bundle",
    "keywords": [ "Doctrine", "Mapping", "Symfony2" ],
    "license": "MIT",
    "homepage": "https://github.com/tomas-pecserke/UniversalMappings",
    "authors": [
        {
            "name": "Tomas Pecserke",
            "email": "tomas@pecserke.eu"
        }
    ],
    "autoload": {
        "psr-0": { "Pecserke\\Component\\UniversalMapping": "" }
    },
    "target-dir": "Pecserke/Component/UniversalMapping",
    "require": {
        "symfony/doctrine-bridge": "~2.3",
        "symfony/dependency-injection": "~2.0",
        "symfony/http-kernel": "~2.0",
        "symfony/config": "~2.0"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
